set title "Agent Total Metabolism Divided By Wealth Created"
set xlabel "Timestep"
set ylabel "Total Metabolism / Wealth Created"
set lt 1 lw 2 lc "black"
set key fixed right bottom
set xtics nomirror
set ytics nomirror
set term pdf
set output "agent_tot_met_div_by_env_wealth_created.pdf"
plot ARGV[1] using 'timestep':'agentTotMetDivByEnvWealthCreated-benthamHalfLookaheadBinary' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 1 pt 0 title 'benthamHalfLookaheadBinary', \
    ARGV[1] using 'timestep':'agentTotMetDivByEnvWealthCreated-benthamHalfLookaheadTop' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 2 pt 1 title 'benthamHalfLookaheadTop', \
    ARGV[1] using 'timestep':'agentTotMetDivByEnvWealthCreated-benthamNoLookaheadBinary' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 3 pt 2 title 'benthamNoLookaheadBinary', \
    ARGV[1] using 'timestep':'agentTotMetDivByEnvWealthCreated-egoisticHalfLookaheadTop' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 4 pt 3 title 'egoisticHalfLookaheadTop', \
    ARGV[1] using 'timestep':'agentTotMetDivByEnvWealthCreated-rawSugarscape' with linespoints pointinterval 100 pointsize 0.75 lt 1 dt 5 pt 4 title 'rawSugarscape'